urxvt*color15: #D0D0D0
!URxvt.font: -*-*-*-*-13-*-*-*-*-*-*-*
!URxvt.boldFont: -*-*-*-*-13-*-*-*-*-*-*-

!urxvt.font: xft: terminus:pixelsize=14,style=bold
!urxvt.font: xft: hack:pixelsize=14,style=bold
!urxvt.font: xft: terminus:pixelsize=10, xft: Inconsolata for Powerline:pixelsize=30
!urxvt.font: xft:Terminus:pixelsize=10,xft:Inconsolata\ for\ Powerline:pixelsize=12
urxvt.font: xft: terminus:pixelsize=10
!urxvt*font: xft: FontAwesome:slant=0:weight=100:pixelsize=15:minspace=True
urxvt*background:       rgba:0000/0000/0000/cccc
URxvt.transparent:   true
URxvt.tintColor:     white
URxvt.shading:       20 
URxvt*saveLines: 512
URxvt*scrollBar: false
!URxvt*background: rgba:0000/0000/0000/ffff
URxvt*foreground: #C0C0C0

!catalog for scripts
!URxvt.perl-lib: /home/brainsburn/.config/urxvt/perl

!links
!https://github.com/muennich/urxvt-perls/raw/master/url-select 
!https://github.com/stepb/urxvt-tabbedex/raw/master/tabbedex 

!URxvt.perl-ext-common:  default,matcher,url-select,tabbedex
!URxvt.matcher.button:   1
!URxvt.urlLauncher:     firefox
!URxvt.underlineURLs:   true

!change tabs colors
!URxvt.tabbed.new-button: no
!URxvt.tabbed.tabbar-fg: 15
!URxvt.tabbed.tabbar-bg: 8
!URxvt.tabbed.tab-fg:    8
!URxvt.tabbed.tab-bg:    14
