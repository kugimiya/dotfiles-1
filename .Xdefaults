! XFT
Xft.hinting: 1
Xft.hintstyle: hintfull
Xft.antialias: false
Xft.dpi: 96

! --- special colors ---

!urxvt*background: #2a0915
!urxvt*foreground: #eea388
urxvt*foreground: #FFFFFF
! --- standard colors ---

! black
!urxvt*color0: #242424
urxvt*color0: #000000
! bright_black
!urxvt*color8: #474747
urxvt*color8: #212121
! red
!urxvt*color1: #8a453c
urxvt*color1: #C00000
! bright_red
!urxvt*color9: #e0bdb8
urxvt*color9: #FF8080
! green
!urxvt*color2: #3c8a45
urxvt*color2: #00C000
! bright_green
!urxvt*color10: #b8e0bd
urxvt*color10: #80FF80
! yellow
!urxvt*color3: #808a3c
urxvt*color3: #C0C000
! bright_yellow
!urxvt*color11: #dce0b8
urxvt*color11: #FFFF88
! blue
!urxvt*color4: #453c8a
urxvt*color4: #0C00C0
! bright_blue
!urxvt*color12: #bdb8e0
urxvt*color12: #99CCFF
! magenta
!urxvt*color5: #8a3c80
urxvt*color5: #C98ACF
! bright_magenta
!urxvt*color13: #e0b8dc
urxvt*color13: #B14CC2
! cyan
!urxvt*color6: #3c808a
urxvt*color6: #00AAAA
! bright_cyan
!urxvt*color14: #b8dce0
urxvt*color14: #9BFFFF
! white
!urxvt*color7: #9b9b9b
urxvt*color7: #FFFFFF
